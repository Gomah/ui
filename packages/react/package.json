{
  "name": "@acme/react",
  "version": "0.0.0",
  "sideEffects": false,
  "main": "./index.tsx",
  "module": "./index.tsx",
  "types": "./index.tsx",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently npm:dev:ladle npm:dev:tsup",
    "dev:ladle": "FORCE_COLOR=1 ladle serve",
    "dev:tsup": "FORCE_COLOR=1 tsup index.tsx --format esm,cjs --external react --dts --watch",
    "build": "yarn build:components && yarn build:ladle",
    "build:components": "tsup index.tsx --format esm,cjs --external react --dts",
    "build:ladle": "ladle build",
    "test": "vitest run"
  },
  "dependencies": {
    "@headlessui/react": "1.7.3",
    "@heroicons/react": "2.0.12",
    "@ladle/react": "2.4.3",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.7",
    "@uiball/loaders": "1.2.6",
    "autoprefixer": "10.4.12",
    "clsx": "1.2.1",
    "framer-motion": "7.5.2",
    "postcss": "8.4.17",
    "tailwind-merge": "1.6.1",
    "tailwindcss": "3.1.8"
  },
  "devDependencies": {
    "@acme/eslint-config-custom": "*",
    "@acme/tailwind-preset": "*",
    "@acme/tokens": "*",
    "@acme/tsconfig": "*",
    "@swc/core": "1.3.4",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "concurrently": "7.4.0",
    "eslint": "8.24.0",
    "jsdom": "20.0.1",
    "prettier-plugin-tailwindcss": "0.1.13",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "6.2.3",
    "typescript": "4.8.4",
    "vitest": "0.23.4"
  }
}
